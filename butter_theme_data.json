  "content": {
    "hero": {
      "title": "Run more engaging sessions. Get better outcomes.",
      "subtitle": "Effortlessly plan, run and recap super-engaging workshops, trainings, and meetings with Butter's built-in agenda planner, integrated collaboration tools and auto-generated recaps—all in one place.",
      "cta": {
        "primary": "Sign up for free",
        "secondary": "Sign up with Google"
      }
    },
    "features": [
      {
        "title": "Run",
        "subtitle": "Say goodbye to Zoom fatigue and juggling countless apps!",
        "description": "Keep everyone engaged and focused on one window - and one outcome. No other tools required.",
        "tools": [
          "Poll",
          "Butterboards",
          "Flashcards",
          "Miro",
          "Google docs",
          "Video",
          "Presentation"
        ]
      },
      {
        "title": "Recap",
        "subtitle": "Never lose a takeaway again",
        "description": "Butter lets you find, remember, and share outcomes in seconds.",
        "features": [
          "AI Summary",
          "Recording",
          "Session GIF's",
          "Feedback",
          "Resource & Links",
          "Poll results"
        ]
      },
      {
        "title": "COLLABORATE",
        "subtitle": "Save time and keep things consistent with workspaces",
        "description": "Your team will thank you for it!"
      }
    ],
    "testimonials": {
      "title": "BUTTER LOVIN'",
      "subtitle": "Loved by facilitators everywhere",
      "description": "Trust their words, not just ours"
    },
    "templates": {
      "title": "Templates",
      "subtitle": "Steal our templates and get started quick",
      "description": "Get inspired by expert-made templates from our community",
      "categories": [
        "Workshops",
        "Trainings",
        "Meetings",
        "Townhall & All-hands",
        "Agile sessions",
        "Community events"
      ]
    },
    "faq": {
      "title": "FAQ",
      "questions": [
        {
          "question": "Why should I use Butter?",
          "answer": "Other video conferencing tools aren't built for collaboration. They're built for conversations. Butter, on the other hand, is built for planning, running, and recapping engaging and interactive collaborative sessions."
        },
        {
          "question": "What types of sessions is Butter best for?",
          "answer": "Butter is a tool for facilitating highly-collaborative sessions like workshops, training sessions, bootcamps, courses, events, interactive webinars, social events, or any other session where you need to keep people energized and productive."
        },
        {
          "question": "Is Butter free?",
          "answer": "We have a free plan that you can use forever — it lets you run sessions of up to 60 minutes with 20 participants max."
        }
      ]
    }
